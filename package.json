{
  "name": "p3g",
  "version": "0.3.0",
  "description": "Global dependency workspace manager for Bun",
  "type": "module",
  "bin": {
    "p3g": "./dist/p3g.js"
  },
  "scripts": {
    "dev": "bun run --watch p3g.ts",
    "start": "bun run p3g.ts",
    "clean": "rm -rf node_modules bun.lock bun.lockb && bun pm cache clean",
    "lint": "eslint . --ext .ts --fix",
    "lint:check": "eslint . --ext .ts",
    "lint:fix": "bun run fix-eslint.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prebuild": "",
    "build": "bun build p3g.ts --outdir dist --target bun",
    "test:integration": "pwsh -File test-integration.ps1 -All",
    "test:integration:windows": "pwsh -File test-integration.ps1 -Windows",
    "test:integration:wsl": "pwsh -File test-integration.ps1 -WSL",
    "test:integration:linux": "bash test-integration.sh",
    "test:readline": "bun run packages/readline/test.ts"
  },
  "keywords": [
    "bun",
    "workspace",
    "dependency-cache",
    "global-workspace",
    "p3g",
    "suissera"
  ],
  "author": "Suissa <inmybox@suissai.dev>",
  "license": "MIT",
  "engines": {
    "bun": ">=1.1.0"
  },
  "files": [
    "dist",
    "packages",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suissa/p3g.git"
  },
  "bugs": {
    "url": "https://github.com/suissa/p3g/issues"
  },
  "homepage": "https://github.com/suissa/p3g#readme",
